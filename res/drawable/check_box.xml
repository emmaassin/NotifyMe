<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
          
    <item android:state_checked="true" 
      android:drawable="@drawable/cbchecked" /> <!-- checked --> 
	<item android:state_checked="false" 
      android:drawable="@drawable/cbunchecked" /> <!-- unchecked --> 
	<item android:state_focused="true"
      android:drawable="@drawable/cbchecked" />    <!--on focus--> 
	<item android:drawable="@drawable/cbunchecked" /> <!-- default --> 

</selector>
